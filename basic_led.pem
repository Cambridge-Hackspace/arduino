//basic led arduino code

int red = 3;
int green = 5;
int blue = 6;

void setup()  { 
  pinMode(red, OUTPUT);
  pinMode(green, OUTPUT);
  pinMode(blue, OUTPUT);
} 

void loop()  { 

  //turn leds on and off
  digitalWrite(red, 1);
  digitalWrite(green, 0);
  digitalWrite(blue, 1);
  
